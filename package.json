  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:docker": "docker-compose up --build -V",
    "start:local": "NODE_ENV=local TZ=UTC nest start --debug 0.0.0.0:9229 --watch ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migration:create": "NODE_ENV=local npm run typeorm:cli -- migration:generate -n",
    "db:migration:run": "npm run typeorm:cli -- migration:run",
    "db:migration:show": "npm run typeorm:cli -- migration:show",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli"
  },
